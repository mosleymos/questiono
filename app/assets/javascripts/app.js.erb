// Application angular
angular.module('questionoapp', ['ngRoute', 'ngResource', 'restangular'])

// CONTROLLERS 

.controller('QuestionListCtrl',['$scope', 'Restangular', function($scope, Restangular){
    $scope.questions = Restangular.all('questions.json').getList().$object ;
}])

// Controller en cours de travail
.controller('QuestionCreateCtrl', ['$scope', '$log', function($scope){
    $scope.createQuestion = function(question){
        $log.info(question);
    }
}]);

